{
    "presets": [
        [
            "@babel/preset-env", {
            "targets": [
                "last 2 versions",
                "not dead",
                "not < 2%",
                "not ie 11"
                ],
            "useBuiltIns": "entry"
            }
        ], 
        "@babel/react",
        "@babel/typescript"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}