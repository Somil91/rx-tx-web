{
	"defaultSeverity": "warning",
	"extends": [
        "tslint:latest", 
		"tslint-react",
		"tslint-config-prettier"
	],
	"jsRules": {},
	"rules": {
		"prettier": [true, "./.prettierrc"],
		"interface-name": false,
		"member-access": false
	},
	"rulesDirectory": ["tslint-plugin-prettier"],
	"linterOptions": {
		"exclude": ["**/node_modules/**"]
	}
}